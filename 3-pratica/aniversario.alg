algoritmo "aniversario"
var 
    ano_atual, ano_nasc, idade: inteiro

inicio
    escreva("Em que ano n√≥s estamos? ")
    leia(ano_atual)
    escreval(ano_atual)
    escreva("Em que ano eu nasci? ")
    leia(ano_nasc)
    escreval(ano_nasc)
    idade <- ano_atual - ano_nasc
    escreva("Isso significa que eu tenho, ", idade, " anos de vida.")
fimalgoritmo